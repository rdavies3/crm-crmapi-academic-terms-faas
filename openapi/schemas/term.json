{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "ASU Salesforce Enterprise Academic Term Object",
    "title": "Term__c",
    "type": "object",
    "properties": {
        "termId": {
            "type": "string",
            "description": "Term unique Id",
            "title": "Id",
            "example": "a0Jd000000U7lzDEAR"
        },
        "aidYear": {
            "type": "string",
            "title": "Aid_Year__c",
            "description": "Financail Aid year associated with term.",
            "example": "2025"
        },
        "name": {
            "type": "string",
            "title": "Name",
            "description": "The term name.",
            "example": "2025 Fall"
        },
        "ownerId": {
            "type": "string",
            "title": "OwnerId",
            "description": "Relationship record owner Id.",
            "example": "005d0000002dYZgAAM"
        },
        "ownerName": {
            "type": "string",
            "title": "Owner.Name",
            "description": "Relationship record owner name.",
            "example": "API ASU DBAMP"
        },
        "sessionAcCloseDate": {
            "title": "Session_A_C_Close_Date__c",
            "description": "Sessions A or C close date.",
            "type": "string",
            "format": "date"
        },
        "sessionAcStartDate": {
            "title": "Session_A_C_Start_Date__c",
            "description": "Sessions A or C start date.",
            "type": "string",
            "format": "date"
        },
        "sessionBCloseDate": {
            "title": "Session_A_C_Close_Date__c",
            "description": "Session B close date.",
            "type": "string",
            "format": "date"
        },
        "sessionBStartDate": {
            "title": "Session_A_C_Start_Date__c",
            "description": "Session B start date.",
            "type": "string",
            "format": "date"
        },
        "semester": {
            "type": "string",
            "title": "Semester__c",
            "description": "Spring, Fall or Summer semesters",
            "example": "Fall"
        },
        "strm": {
            "type": "string",
            "title": "PeopleSoft_Code__c",
            "description": "The person Id tracked on this Relationship Record. Used in Contact to Account and Contact to Contact Relationships.",
            "example": "003d000003IHmcSAAT"
        },
        "termBeginDate": {
            "title": "Session_A_C_Close_Date__c",
            "description": "Term begin date.",
            "type": "string",
            "format": "date"
        },
        "termEndDate": {
            "title": "Session_A_C_Start_Date__c",
            "description": "Term end date.",
            "type": "string",
            "format": "date"
        },
        "termStatus": {
            "type": "boolean",
            "title": "Term_Status__c",
            "description": "Is this term activated?",
            "example": false
        },
        "year": {
            "type": "string",
            "title": "Year__c",
            "description": "Academic Year.",
            "example": "2025"
        }
    }
}
